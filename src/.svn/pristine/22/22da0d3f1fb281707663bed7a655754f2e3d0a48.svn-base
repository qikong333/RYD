.ion-ios-backicon:before {
    text-align: center;
    content: " ";
    background: url("../assets/image/backicon.svg") no-repeat;
    background-size: 100%;
    display: list-item;
    width: 100%;
    height: r(70px);
    font-size: 0.78125rem;
}

