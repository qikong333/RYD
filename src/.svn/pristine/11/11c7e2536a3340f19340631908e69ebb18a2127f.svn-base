    page-home {
        //不要头部,加上此段;
        .fixed-content {
            margin-top: 0px!important;
        }
        .scroll-content {
            margin-top: 0px!important;
        }
        /* 头部，轮播图样式 总范围*/
        .header {
            height: r(405px);
            /* 轮播的点点 */
            .roundpicture {
                //轮播点点的大小和位置
                .swiper-pagination-fraction,
                .swiper-pagination-custom,
                .swiper-container-horizontal>.swiper-pagination-bullets {
                    bottom: r(110px);
                    .swiper-pagination-bullet {
                        width: r(13px);
                        height: r(13px);
                    }
                }
                //选中的颜色
                .swiper-pagination-bullet-active {
                    background: rgb(255, 255, 255);
                }
                .hot_all {
                    height: r(405px);
                }
                .slide-zoom {
                    height: r(405px);
                }
                .hot_slide {
                    height: r(405px);
                    img {
                        width: 100%;
                        height: 100%;
                    }
                }
            }
            /* 轮播的字边 */
            .toptitle {
                padding: 0px;
                top: r(285.5px);
                width: 100%;
                height: r(120px);
                z-index: 9999;
                position: absolute;
                line-height: r(30px);
                background-color: rgba(0, 0, 0, 0.5);
                i {
                    font-size: r(31px);
                }
            }
            .leftTitle {
                font-size: r(23px);
                text-align: center;
                color: #ffffff;
                p {
                    padding-top: r(20px);
                }
            }
            .leftTitle::after {
                content: '';
                /*CSS伪类用法*/
                position: absolute;
                /*定位背景横线的位置*/
                top: 18%;
                right: 0;
                background: #ffffff;
                /*宽和高做出来的背景横线*/
                width: r(1px);
                height: r(60px);
            }
            .rightTitle {
                font-size: r(23px);
                text-align: center;
                color: #ffffff;
                p {
                    padding-top: r(20px);
                }
            }
        }
        /* icon图标 */
        .nav {
            background-color: white;
            border-bottom: solid r(20px) #ededed;
            // height: r(152px);
            .navtitle {
                padding-top: r(10px);
                padding-bottom: r(10px);
                font-size: r(26px);
                text-align: center;
                color: rgb(50, 50, 50);
                img {
                    height: r(64px);
                }
            }
        }
        .logo {
            font-size: r(32px);
        }
        .foot {
            height: r(200px);
            width: 100%;
            bottom: 0px;
            color: #909090;
            background-color: #ededed;
            padding: r(15px) r(30px) r(15px) r(30px);
            p {
                text-align: center;
                padding-top: r(80px);
                font-size: r(24px);
            }
        }
        ion-content.has-refresher>.scroll-content {
            background-color: #ededed;
        }
    }